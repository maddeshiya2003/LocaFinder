<!--aur req.flash me key(success) wala us variable me store ho jayega -> ["listing are aadded"] 
nahi toh bas success banega but kuch store nahi hoga  -> [] isliye <.%if(success && success.length){%> ye likhe hai -->
<!-- app.js folder me hai -->
<br>
<%if(success && success.length){%>
    <div class="alert alert-success alert-dismissible fade show col-8 offset-2 flash" role="alert">
        <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>

<%if(error && error.length){%>
    <div class="alert alert-danger alert-dismissible fade show col-8 offset-2 flash" role="alert">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>